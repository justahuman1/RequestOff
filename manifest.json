{
  "description": "Unnecessary Javascript is the worst. Control what requests are coming in/out of your browser, per tab, per window, etc.",
  "manifest_version": 2,
  "name": "RequestOff",
  "version": "0.1",
  "homepage_url": "https://www.github.com/justahuman1",
  "permissions": [
    "tabs",
    "menus",
    "*://*/*",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "notifications"
  ],
  "icons": {
    "16": "data/svg/icon2.svg",
    "32": "data/svg/icon2.svg",
    "48": "data/svg/icon2.svg",
    "64": "data/svg/icon2.svg",
    "128": "data/svg/icon2.svg"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
